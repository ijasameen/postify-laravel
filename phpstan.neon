includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
    level: max

    paths:
        - app
        - bootstrap/app.php
        - bootstrap/providers.php
        - config
        - database
        - public
        - routes

    tempDir: /tmp/phpstan
